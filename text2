@startuml
skinparam actor {
	BackgroundColor White
	ArrowColor Black
	BorderColor Black
}
skinparam usecase {
	BackgroundColor White
	ArrowColor Black
	BorderColor Black
}
left to right direction
actor P0 as "Работники"
actor M0 as "Программное средство"
actor P as "Координатор"
actor M as "Интегрированная система управления"
usecase A as "Оформление обучения"
P -- (A)
(A) -- M
P0 <|-- P
M --|> M0
@enduml
